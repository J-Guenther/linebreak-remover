{
  "name": "linebreak-remover",
  "version": "1.0.0",
  "description": "Removes unwanted formatting from text in your clipboard copied from a PDF",
  "scripts": {
    "cleanup": "rmdir /q /s .parcel-cache dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run cleanup && parcel src/index.html",
